

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Diagnosis_Layout.cshtml";
}
@{
    var items = new SelectList(new[]
        { 
           new SelectListItem {Text = "无", Value = "无"},
            new SelectListItem {Text = "曾患", Value = "曾患"},
            new SelectListItem {Text = "现患", Value = "现患"},
        }, "Text", "Value");
}
@{
    var scores = new SelectList(new[]
    {
    new SelectListItem { Text="", Value=""},
    new SelectListItem { Text="0", Value="0"},
    new SelectListItem { Text="1", Value="1"},
    new SelectListItem { Text="2", Value="2"},
    }, "Text", "Value");
}
@{
    var actions = new SelectList(new[]
        {
        new SelectListItem { Text="4", Value="独立完成"},
        new SelectListItem { Text="3", Value="有些困难"},
        new SelectListItem { Text="2", Value="需要帮助"},
        new SelectListItem { Text="1", Value="无法完成"},
        new SelectListItem { Text="0", Value="不知道/不适用"},
        }, "Text", "Value");
