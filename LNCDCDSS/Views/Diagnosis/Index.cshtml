                success: function (data) {
                    var label = document.getElementById('lastScore_1').innerHTML = data[2];
                    var label = document.getElementById('lastScore_2').innerHTML = data[3];
                    var label = document.getElementById('lastScore_3').innerHTML = data[4];
                    var label = document.getElementById('lastScore_4').innerHTML = data[5];
                    var label = document.getElementById('lastScore_5').innerHTML = data[6];
                    var label = document.getElementById('lastScore_6').innerHTML = data[7];
                    var label = document.getElementById('patinfo_name').innerHTML = data[15];
                    var label = document.getElementById('patinfo_sex').innerHTML = data[16];
                    var label = document.getElementById('patinfo_age').innerHTML = data[17];
                    var label = document.getElementById('patinfo_PID').innerHTML = data[19];
                    var label = document.getElementById('patinfo_phone').innerHTML = data[18];
                }
            });

        });
    </script>
